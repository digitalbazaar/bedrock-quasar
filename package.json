{
  "name": "bedrock-quasar",
  "main": "index.js",
  "version": "0.0.1",
  "dependencies": {
    "css-vars-ponyfill": "^1.8.0",
    "quasar-framework": "^0.15.10"
  },
  "bedrock": {
    "browserDependencies": "all",
    "manifest": {
      "quasar-framework": {
        "main": "dist/umd/quasar.mat.umd.js",
        "style": "dist/umd/quasar.mat.css"
      }
    },
    "webpack": {
      "quasar-framework": {
        "resolve": {
          "alias": {
            "quasar-framework$": "dist/umd/quasar.mat.umd.js"
          }
        }
      }
    }
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
